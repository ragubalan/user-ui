{
  "name": "user-ui",
  "version": "0.0.0",
  "author": "Ragubalan Durairaj",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "build": "npm-run-all clean transpile",
    "server": "nodemon ./dist-server/bin/www",
    "dev": "npm-run-all build server",
    "transpile": "babel ./src --out-dir dist-server",
    "clean": "rimraf dist-server"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "js,yaml,html,njk"
  },
  "dependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.3",
    "rimraf": "^4.4.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
